dnsinuse(){
	nmcli dev show | grep IP4.DNS }

findip(){
    suda arp-scan --localnet }

hotspot(){
    sudo /home/bresilla/.sbin/hotspot wlp2s0 wlp2s0 algorithm dyhere024 }

alias speed="wget --output-document=/dev/null http://speedtest.wdc01.softlayer.com/downloads/test500.zip"

portgraph(){
    sudo lsof -n -F | ./lsofgraph | dot -Tjpg > /tmp/a.jpg
}

portmon(){
    sudo lsof -Pnl -i4
}

openports(){
    sudo netstat -tulpn
}

wifis(){
    nmcli -t -f ssid dev wifi
}

myip(){
    dig +short myip.opendns.com @resolver1.opendns.com
}
