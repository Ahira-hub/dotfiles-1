update(){
	sudo apt update
}
upgrade(){
	sudo apt upgrade
}
autoremove(){
	sudo apt autoremove
}
clean(){
	sudo apt clean
}

headphones(){
	pacmd set-sink-port 0 analog-output-headphones
}
speaker(){
	pacmd set-sink-port 0 analog-output-speaker
}

own(){
	sudo chown -R $USER $1
}

exe(){
	chmod +x $1
}

screen_size(){
	xrandr | grep '*' | awk '{print $1;}'
}

script_bin(){
	sudo cp /home/trim/Dots/.scripts/* /usr/local/sbin/
}

script_del(){
	sudo rm /usr/local/sbin/*
}
