update(){
	cat /home/bresilla/pass | sudo -S apt update
}
upgrade(){
	cat /home/bresilla/pass | sudo -S apt upgrade
}
autoremove(){
	cat /home/bresilla/pass | sudo -S apt autoremove
}
clean(){
	cat /home/bresilla/pass | sudo -S apt clean
}

headphones(){
	pacmd set-sink-port 0 analog-output-headphones
}
speaker(){
	pacmd set-sink-port 0 analog-output-speaker
}

own(){
	cat /home/bresilla/pass | sudo -S chown -R $USER $1
}

exe(){
	chmod +x $1
}

screen_size(){
	xrandr | grep '*' | awk '{print $1;}'
}
