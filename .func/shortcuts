distract(){
	if [ $# -eq 0 ]; then
        vim ~/Dots/.assets/.notes/distracted.wiki
    else
    echo $* >> ~/Dots/.assets/.notes/distracted.wiki
    fi
}


wiki(){
	vim ~/Dots/.assets/.notes/index.wiki
}

weather(){
	curl wttr.in
}

setwall(){
	wget -O /tmp/wallpaper.jpg https://source.unsplash.com/3840x2160/?nature
	feh --bg-fill /tmp/wallpaper.jpg
}

